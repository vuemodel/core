{
  "name": "@vuemodel/core",
  "version": "0.0.75",
  "scripts": {
    "build": "tsdown",
    "watch": "tsdown --watch --format=esm",
    "release": "pnpm build && bumpp --tag '@vuemodel/core@v%s' && pnpm publish"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuemodel/core.git"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Luke Diebold",
  "license": "MIT",
  "description": "Core repository for contracts and drivers of VueModel",
  "bugs": {
    "url": "https://github.com/vuemodel/core/issues"
  },
  "homepage": "https://github.com/vuemodel/core#readme",
  "keywords": [
    "vue",
    "orm",
    "model"
  ],
  "dependencies": {
    "@vuemodel/core": "^0.0.38",
    "debounce": "^2.2.0",
    "deep-equal": "^2.2.3",
    "deepmerge-ts": "^5.1.0",
    "just-clone": "^6.2.0",
    "just-diff": "^6.0.2",
    "just-index": "^4.2.0",
    "just-omit": "^2.2.0",
    "just-remove": "^3.2.0",
    "just-safe-set": "^4.2.1",
    "uuid": "^9.0.1"
  },
  "peerDependencies": {
    "pinia": "^2.x",
    "pinia-orm": "1.10.x",
    "vue": "^3.x"
  },
  "devDependencies": {
    "@types/deep-equal": "^1.0.4",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vuemodel/sample-data": "workspace:^",
    "bumpp": "^9.11.1",
    "eslint": "^8.57.1",
    "tsdown": "^0.12.3",
    "tslib": "^2.8.1",
    "typescript": "~5.8.3"
  }
}
