{
  "name": "@vuemodel/pinia-local-storage",
  "version": "0.0.26",
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "watch": "nodemon --ext ts,js --exec unbuild",
    "release": "pnpm build && bumpp --tag '@vuemodel/pinia-local-storage@v%s' && pnpm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuemodel/core.git"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Luke Diebold",
  "license": "MIT",
  "description": "Core repository for contracts and drivers of VueModel",
  "bugs": {
    "url": "https://github.com/vuemodel/core/issues"
  },
  "homepage": "https://github.com/vuemodel/core#readme",
  "keywords": [
    "vue",
    "orm",
    "model"
  ],
  "dependencies": {
    "idb-keyval": "^6.2.1",
    "just-clone": "^6.2.0",
    "just-index": "^4.2.0",
    "pinia-orm-helpers": "^0.0.16"
  },
  "peerDependencies": {
    "@vuemodel/core": "^0.0.x",
    "pinia": "^2.x",
    "pinia-orm": "1.7.x",
    "vue": "^3.x"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "bumpp": "^9.2.0",
    "eslint": "^8.51.0",
    "nodemon": "^3.0.1",
    "tslib": "^2.6.2",
    "typescript": "~5.5.4",
    "unbuild": "^3.0.0-rc.7"
  }
}