{
  "name": "@vuemodel/pinia-local-storage",
  "version": "0.0.8",
  "scripts": {
    "jiti-fix": "find ./dist -type f -exec sed -i 's|file://||g' {} \\;",
    "build": "unbuild",
    "dev": "unbuild --stub && pnpm jiti-fix",
    "release": "pnpm build && bumpp --tag '@vuemodel/pinia-local-storage@v%s' && pnpm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuemodel/core.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Luke Diebold",
  "license": "MIT",
  "description": "Core repository for contracts and implementations of VueModel",
  "bugs": {
    "url": "https://github.com/vuemodel/core/issues"
  },
  "homepage": "https://github.com/vuemodel/core#readme",
  "keywords": [
    "vue",
    "orm",
    "model"
  ],
  "dependencies": {
    "@vuemodel/core": "workspace:^",
    "idb-keyval": "^6.2.1",
    "pinia-orm-helpers": "^0.0.11"
  },
  "peerDependencies": {
    "pinia": "^2.x",
    "pinia-orm": "^1.x",
    "vue": "^3.x"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "bumpp": "^9.2.0",
    "eslint": "^8.51.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0"
  }
}
